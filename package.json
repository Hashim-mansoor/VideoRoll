{
    "name": "video-roll",
    "version": "1.0.6",
    "description": "a extension that can help you to rotate a video",
    "author": "Gomi",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "watch:inject": "parcel watch src/inject/injected.ts",
        "watch:background": "parcel watch src/background/background.ts",
        "start": "npm run clean && npm run copy:static && parcel watch src/index.html src/inject/index.ts src/background/index.ts",
        "build:popup": "parcel build src/index.html --no-source-maps --no-content-hash",
        "build:inject": "parcel build src/inject/index.ts --no-source-maps --no-content-hash",
        "build:background": "parcel build src/background/index.ts --no-source-maps --no-content-hash",
        "build": "npm run build:popup && npm run build:inject && npm run build:background",
        "copy:static": "copyfiles -u 1 src/icons/* src/manifest.json dist",
        "clean": "rimraf dist",
        "dist": "npm run clean && npm run copy:static && npm run build"
    },
    "source": [
        "src/index.html",
        "src/inject/index.ts",
        "src/background/index.ts"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gxy5202/video-roll.git"
    },
    "keywords": [
        "rotate video",
        "video rotater",
        "transform video"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gxy5202/video-roll/issues"
    },
    "homepage": "https://github.com/gxy5202/video-roll#readme",
    "dependencies": {
        "@parcel/transformer-typescript-types": "^2.7.0",
        "@vicons/ionicons5": "^0.11.0",
        "typescript": "^4.5.2",
        "vant": "4.0.0-rc.0",
        "vue": "^3.2.26"
    },
    "devDependencies": {
        "@parcel/reporter-bundle-analyzer": "^2.4.1",
        "@parcel/transformer-vue": "^2.4.1",
        "@types/chrome": "^0.0.196",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "@vue/compiler-sfc": "^3.2.26",
        "copyfiles": "^2.4.1",
        "eslint": "^8.23.0",
        "jest": "^28.1.3",
        "less": "^4.1.2",
        "parcel": "^2.4.1",
        "parcel-namer-hashless": "^1.0.3",
        "process": "^0.11.10"
    }
}
