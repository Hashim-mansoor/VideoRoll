{
    "name": "video-roll",
    "version": "1.1.2",
    "description": "A extension that can help you to rotate a video",
    "author": "Gomi",
    "scripts": {
        "test": "jest",
        "watch:inject": "parcel watch src/inject/injected.ts",
        "watch:background": "parcel watch src/background/background.ts",
        "preview": "parcel src/popup/index.html",
        "dev": "npm run clean && npm run copy:static && parcel watch src/popup/index.html src/inject/index.ts src/background/index.ts --no-cache",
        "build:popup": "parcel build src/popup/index.html --dist-dir dist/popup/ --no-source-maps --no-content-hash",
        "build:inject": "parcel build src/inject/index.ts --dist-dir dist/inject --no-source-maps --no-content-hash",
        "build:background": "parcel build src/background/index.ts --dist-dir dist/background --no-source-maps --no-content-hash",
        "build": "npm run clean && npm run copy:static && npm run build:popup && npm run build:inject && npm run build:background && npm run bin",
        "copy:static": "copyfiles -u 1 src/icons/* src/manifest.json dist",
        "bin": "node build/bin.js",
        "clean": "rimraf dist && rimraf chromiumExt.zip && rimraf firefoxExt.zip",
        "start": "npm run dev",
        "precommit": "npm run test"
    },
    "source": [
        "src/popup/index.html",
        "src/inject/index.ts",
        "src/background/index.ts"
    ],
    "parcel-namer-hashless": {
        "mode": "all"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gxy5202/video-roll.git"
    },
    "keywords": [
        "rotate video",
        "video rotater",
        "transform video",
        "html5",
        "video"
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/gxy5202/video-roll/issues"
    },
    "homepage": "https://github.com/gxy5202/video-roll#readme",
    "dependencies": {
        "@vant/touch-emulator": "^1.4.0",
        "@vicons/ionicons5": "^0.11.0",
        "vant": "4.0.0-rc.0",
        "vue": "^3.2.41"
    },
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@babel/plugin-transform-modules-commonjs": "^7.18.6",
        "@parcel/config-default": "^2.8.3",
        "@parcel/reporter-bundle-analyzer": "^2.4.1",
        "@parcel/transformer-less": "^2.8.3",
        "@parcel/transformer-vue": "^2.8.3",
        "@parcel/validator-typescript": "^2.8.3",
        "@types/chrome": "^0.0.196",
        "@types/jest": "^29.0.3",
        "@types/node": "^18.8.4",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "@vue/babel-plugin-jsx": "^1.1.1",
        "@vue/compiler-sfc": "^3.2.26",
        "chalk": "^4.1.2",
        "compressing": "^1.8.0",
        "copyfiles": "^2.4.1",
        "eslint": "^8.23.0",
        "husky": "^8.0.3",
        "jest": "^29.0.3",
        "jest-environment-jsdom": "^29.0.3",
        "less": "^4.1.2",
        "parcel": "^2.8.3",
        "parcel-namer-hashless": "^1.0.4",
        "process": "^0.11.10",
        "ts-jest": "^29.0.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
